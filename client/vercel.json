{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/pages/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/app/$1"
      }
    ],
    "env": {
      "MY_ENV_VARIABLE": "value"
    }
  }
  